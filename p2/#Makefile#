########################################################
CC=gcc
CFLAGS= -g -Wall -pedantic -ansi
EJS = pruebaMorfo
########################################################
OBJECTSP3_E1 = p lex.yy.o
########################################################

all: $(EJS) clear

pruebaMorfo: $(OBJECTSP3_E1)
	$(CC) -o pruebaMorfo $(OBJECTSP3_E1)

pruebaMorfo.o: pruebaMorfo.c tokens.h
	$(CC) -c pruebaMorfo.c

lex.yy.o: lex.yy.c
	$(CC) -c lex.yy.c

clean:
	rm -rf *.o $(EJS)
